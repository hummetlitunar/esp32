mixed-port: 7890
allow-lan: false
mode: rule
log-level: info

proxies:
  - name: "Dummy-Proxy"
    type: socks5
    server: 127.0.0.1
    port: 1080

proxy-groups:
  - name: "Proxy"
    type: select
    proxies:
      - "Dummy-Proxy"

rules:
  - MATCH,Proxy

experimental:
  ignore-resolve-fail: true

tun:
  enable: true
  stack: system
  auto-route: true
  auto-detect-interface: true
  ttl: 65
